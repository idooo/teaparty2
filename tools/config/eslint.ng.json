{
    "globals": {
        "Sizzle": true,
        "angular": true,
        "is": true
    },
    "env": {
        "browser": true
    },
    "ecmaFeatures": {
        "templateStrings": true,
        "blockBindings": true,
        "forOf": true,
        "defaultParams": true
    },
    "rules": {
        "strict": false,
        "semi": false,
        "no-unused-vars": false,
        "no-use-before-define": [2, "nofunc"],
        "curly": 0,
        "quotes": [2, "single", "avoid-escape"],
        "no-underscore-dangle": 0
    }
}